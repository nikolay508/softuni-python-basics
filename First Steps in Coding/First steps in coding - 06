nailon = float(input())
boq = float(input())
razreditel = int(input())
hours_workers = int(input())
sum_nai = (nailon * 1.5) + 3
first_sum_boq = boq * 14.5
second_sum_boq = 0.1 * first_sum_boq
final_sum_boq = first_sum_boq + second_sum_boq
sum_raz = razreditel * 5
all_materials = sum_nai + final_sum_boq + sum_raz + 0.4
workers_salary = (0.3 * all_materials) * hours_workers
print(all_materials + workers_salary)
