book_wanted = input()
book_search = input()
amount = 0

while True:
    if book_wanted == book_search:
        print(f"You checked {amount} books and found it.")
        break
    elif book_search == "No More Books":
        print("The book you search is not here!")
        print(f"You checked {amount} books.")
        break
    amount += 1
    book_search = input()
