number = int(input())
points = 0
if number <= 100:
    points = points + 5
elif 1000 > number > 100:
    points = 0.2 * number
else:
    points = 0.1 * number

if number % 2 == 0:
    points = points + 1

if number % 10 == 5:
    points = points + 2

all_sum = number + points

print(points)
print(all_sum)
